{"name":"mergeJoin",
"description": "A user-defined control-flow pattern that depicts a merge Join between two sorted data inputs in an ETL flow",
"pattern":[
	"splitFlow":[
		{"flow1":[
			"repeat": "=1",
			"sequence":[
				"s1": {"name": "optype", "values":["value": "Sort"]}]]},
		{"flow2":[
			"repeat": "=1",
			"sequence":[
				"s1": {"name": "optype", "values":["value": "Sort"]}]]}
		],
	"sequence": [
		"s1": {"name":"optype", "values": ["value": "Join", "value": "LeftOuterJoin"]}]
],
"bpmnElement":[{"name":"subprocess", "attributes":[{"name":"completionQuantity", "value":"1"},
												{"name":"id", "value":"$id"},
												{"name":"isForCompensation", "value":"false"},
												{"name":"name", "value":"$create"}
												{"name": "startQuantity", "value":"1"},
												{"name": "triggeredByEvent", "value":"false"}]}]
},

{"name":"recoveryPoint",
"description": "A user-defined control-flow pattern that 
				identifies check-pointing in an ETL flow",
"pattern":[{
	"sequence":[
		"s1": {"name": "optype", "values": 
								["value": "Splitter", "value": "Router"]}],
	"splitFlow":[
		{"flow1":[
			"repeat": "=1",
			"sequence":[{
				"s1":{"name":"*", "values": ["value": "whiteList"]},
				"s2":{"name":"type", "values": ["value": "Datastore"]}]}]}]
}],
"bpmnElement":[
	{"name":"subprocess", 
	 "attributes":[{"name":"name", "value":"$create"},
				{"name":"id", "value":"$id"},
				{"name":"completionQuantity", "value":"1"},
				{"name":"isForCompensation", "value":"false"},
				{"name": "startQuantity", "value":"1"},
				{"name": "triggeredByEvent", "value":"false"}]}],
"whiteList":[{"name": "optype", "values": ["value": "UserDefinedFunction"]}]
},

{"name":"compensation",	
"description": "A user-defined control-flow pattern to depict 
				compensation action operations in an ETL flow",
"pattern":[{
	"sequence": [
		{"s1": {"name": "optype", "values": ["value": "Splitter", "value": "Router"]}}],
	"splitFlow":[
		{"flow1":[
			"repeat": "=1",
			"sequence":[
				{"s1": {"name":"#1", "values": ["value": "whiteList"]}},
				{"s2": {"name":"type", "values":["value": "Datastore"]}}]]}]
}],
"bpmnElement":[
	{"name":"subprocess", 
	 "attributes":[{"name":"name", "value":"$create"},
				{"name":"id", "value":"$id"},
				{"name":"completionQuantity", "value":"1"},
				{"name":"isForCompensation", "value":"true"},
				{"name": "startQuantity", "value":"1"},
				{"name": "triggeredByEvent", "value":"false"}]}], 
"whiteList": [{"name": "optype", "values": ["value": "UserDefinedFunction"]}]
},

{"name":"subprocess",
"description": "A user-defined ETL flow pattern that groups parts on the flow that can be pipelined",
"pattern":[{
	"sequence":[							
		{"s1":{"name": "*", "values":["value": "whiteList"]}}]
}],
"bpmnElement":[{"name":"subprocess", "attributes":[{"name":"completionQuantity", "value":"1"},
												{"name":"id", "value":"$id"},
												{"name":"isForCompensation", "value":"false"},
												{"name":"name", "value":"$create"}
												{"name": "startQuantity", "value":"1"},
												{"name": "triggeredByEvent", "value":"false"}]}],
"whiteList":[{"name":"optype", "values": ["value": "Router", "value":"Splitter", "value":"Merger", "value":"Voter", 
				"value":"Filter", "value":"AttributeAddition", "value":"Rename", "value":"Project"]},
			{"name": "implementationType", "values": ["value": "MERGE", "value": "LEFTOUTERMERGE"]}],
"blackList":[{"name": "optype", "values": ["value":"Sort", "value":"Distinct", "value":"TopK",  "value":"UserDefinedFunction", "value":"Union"]},
				{"name": "implementationType", "values": ["value": "HASH", "value":"LEFTOUTERHASH"]}]
},

{"name":"replication",
"description": "A user-defined control-flow pattern that 
				identifies replication in an ETL flow",
"pattern":[
	"sequence":[
		"s1": {"name": "optype", "values": 
								["value": "Splitter", "value": "Router"]}],
	"splitFlow":[
		{"flow1":[
			"repeat": ">1",
			"sequence":[
				"s1":{"name":"*", "values": ["value": "*"]}]]}]
],
"bpmnElement":[
	{"name":"subprocess", 
	 "attributes":[{"name":"name", "value":"$create"},
				{"name":"id", "value":"$id"},
				{"name":"completionQuantity", "value":"1"},
				{"name":"isForCompensation", "value":"false"},
				{"name": "startQuantity", "value":"1"},
				{"name": "triggeredByEvent", "value":"false"}]},
	{"name":"multiInstanceLoopCharacteristics",
	"attributes":[{"name": "behavior", "value": "All"}, 
				{"name":"isSequential", "value":"false"}]}]
},


]
